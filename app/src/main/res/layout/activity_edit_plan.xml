        android:layout_height="match_parent"
        app:layout_constraintTop_toBottomOf="@id/title0"
        app:layout_constraintLeft_toLeftOf="parent">
        <TextView
            android:id="@+id/debug"
            android:textSize="30sp"
            android:text="debug"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />

    </LinearLayout>
